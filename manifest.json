{
    "manifest_version": 3,
    "name": "QueueTube",
    "description": "Queue up videos with your friends and watch together live!",
    "version": "0.0.1",
    "icons": {
        "16": "assets/logo-16.png",
        "48": "assets/logo-48.png",
        "128": "assets/logo-128.png"
    },
    "permissions": [],
    "host_permissions": [
        "*://*/*"
    ],
    "content_scripts": [{
        "js": ["build/index.js"],
        "css": ["build/index.css"],
        "matches": [
            "https://youtube.com/*",
            "https://www.youtube.com/*"
        ]
    }]
}
